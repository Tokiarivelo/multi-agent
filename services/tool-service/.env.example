# Environment
NODE_ENV=development

# Server
PORT=3003

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/multi_agent_db

# Tool Execution
TOOL_EXECUTION_TIMEOUT=30000
MAX_TOOL_MEMORY_MB=128
ENABLE_FILE_OPERATIONS=true
ALLOWED_DOMAINS=*

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=30

# Security
SANDBOX_ENABLED=true
